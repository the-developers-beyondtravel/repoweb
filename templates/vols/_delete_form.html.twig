<form method="post" action="{{ path('app_vols_delete', {'id': vol.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vol.id) }}">
    <button class="btn btn-round btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>
</form>
