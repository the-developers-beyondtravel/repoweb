<!DOCTYPE html>

<title>Chambres</title>

{% block body %}
    <h1>Chambres</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ chambre.id }}</td>
            </tr>
            <tr>
                <th>TypeChambre</th>
                <td>{{ chambre.typeChambre }}</td>
            </tr>
            <tr>
                <th>NbrLit</th>
                <td>{{ chambre.nbrLit }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ chambre.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ chambre.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_chambres_index') }}">back to list</a>

    <a href="{{ path('app_chambres_edit', {'id': chambre.id}) }}">edit</a>

    {{ include('chambres/_delete_form.html.twig') }}
