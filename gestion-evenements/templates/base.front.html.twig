		            </li>
					<li>
		                <a href="#"><span class="title">Vol</span></a>
		            </li>
					<li>
		                <a href="#"><span class="title">Offre</span></a>
		            </li>
{% if app.user %}	
					<li>
		                <a href="#"><span class="title">Reclamation</span></a>
		            </li>						


		        </ul>
		        
				
				

			                
						
		
					
						<ul class="header_user_notif pull-right dn-smd">	
							
							<li class="user_setting">
						<div class="dropdown">
						{% if app.user.ImgName is empty %}	
	                		<a class="btn dropdown-toggle" href="#" data-toggle="dropdown"><img class="rounded-circle" src="images/team/e1.png" alt="e1.png"></a>
							{% else %}
							<a class="btn dropdown-toggle" href="#" data-toggle="dropdown"><img class="rounded-circle" src="uploads/images/user/{{ app.user.ImgName }}" alt="e1.png"></a>
							{% endif %}
						    <div class="dropdown-menu">
						    	<div class="user_set_header">
                                {% if app.user.ImgName is empty %}	
						    		<img class="float-left rounded-circle" src="images/team/e1.png" alt="e1.png">
									{% else %}
									<img class="float-left rounded-circle" src="uploads/images/user/{{ app.user.ImgName }}" alt="e1.png">
									{% endif %}
									
							    	<p>{{ app.user.firstname }} {{ app.user.lastname }}</p>
						    	</div>
						    	<div class="user_setting_content">
									<a class="dropdown-item active" href="/settings">My Profile</a>
									<a class="dropdown-item" href="{{ path('app_logout') }}">Log out</a>
						    	</div>
						    </div>
						</div>
			        </li>    
			</ul>
			{% else %}
			<ul class="sign_up_btn pull-right dn-smd mt20">
				<li><a href="/login" class="btn flaticon-user" > <span class="dn-lg">Login</span></a></li>
 				</ul><!-- Button trigger modal -->
				{% endif %}
			</nav>
		</div>
        </header>
	<!-- Main Header Nav For Mobile -->
		<nav id="menu" class="stylehome1">
			<ul>
